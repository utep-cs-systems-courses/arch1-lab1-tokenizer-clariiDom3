OBJS = tokenizer.o history.o uiMain.o
# complier flags
CFLAGS= -g -O3

#first target is built by default
all: tokenizer

tokenizer: $(OBJS)
	cc -o tokenizer $(CFLAGS) $(OBJS)

$(OBJS): tokenizer.h history.h

# deletes files generated by compilation
clean:
	rm -f *.o tokenizer

run: tokenizer
	./tokenizer

